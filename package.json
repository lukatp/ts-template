    "@typescript-eslint/parser": "^7.12.0",
    "eslint": "8",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.3.1",
    "typescript": "^5.4.5"
  }
